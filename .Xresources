! Font rendering
Xft.dpi:        96
Xft.autohint:   0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting:    1
Xft.antialias:  1
Xft.rgba:       rgb

! urxvt settings
URxvt*font:             xft:Menlo:size=11:style=Regular
URxvt*boldFont:         xft:Menlo:size=11:style=Regular
! URxvt*font:             -misc-termsyn-medium-*-normal-*-14-*-*-*-*-*-iso8859-1
! URxvt*boldFont:         -misc-termsyn-medium-*-normal-*-14-*-*-*-*-*-iso8859-1
URxvt*letterSpace:      -1
URxvt*scrollBar:        false
URxvt*depth:            32
URxvt*background:       rgba:0000/0000/0000/c800
URxvt*intensityStyles:  false

URxvt*saveLines:    1024

! rofi settings
rofi*font:          Misc Termsyn 16
